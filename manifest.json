{
	"name": "Chrome Sniffer",
	"icons": {
		"48": "icon48.png",
		"128": "icon128.png"
	},
	"version": "0.1.3",
	"description": "Detect web frameworks and javascript libraries run on browsing website.",
	"background_page": "background.html",
	"page_action": {
		"default_icon": "icon.png",
		"default_title": "Click for more details.",
		"popup": "popup.html"
	},
	"content_scripts": [{
		"matches": ["http://*/*","https://*/*"],
		"js": ["inject.js"],
		"run_at": "document_end"
    }],
	"permissions": [
		"tabs"
	]
}