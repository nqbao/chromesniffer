{
	"name": "Chrome Sniffer",
	"manifest_version": 2,
	"icons": {
		"48": "icon48.png",
		"128": "icon128.png"
	},
	"version": "0.2.13",
	"description": "Detect web applications and javascript libraries run on browsing website.",
	"background_page": "background.html",
	"page_action": {
		"default_icon": "icon.png",
		"default_title": "Click for more details.",
		"popup": "popup.html"
	},
	"content_scripts": [
		{
			"matches": ["http://*/*","https://*/*"],
			"js": ["main.js"]
		}
	],
	"permissions": [
		"tabs",
        "http://*/*",
        "https://*/*"
	]
}
