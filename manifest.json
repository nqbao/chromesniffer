{
	"name": "Chrome Sniffer",
	"manifest_version": 2,
	"icons": {
		"48": "icon48.png",
		"128": "icon128.png"
	},
	"version": "0.2.13",
	"description": "Detect web applications and javascript libraries run on browsing website.",
	"background": {
		"scripts": ["common.js", "engine.js", "apps.js", "background.js"],
		"persistent": true
	},
	"page_action": {
		"default_icon": "icon.png",
		"default_title": "Click for more details.",
		"default_popup": "popup.html"
	},
	"content_scripts": [
		{
			"matches": ["http://*/*","https://*/*"],
			"js": ["detector.js"]
		}
	],
	"permissions": [
		"tabs",
		"webRequest",
		"http://*/*",
		"https://*/*"
	]
}
